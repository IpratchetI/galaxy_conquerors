{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:client": "vite",
    "dev": "cross-env NODE_ENV=development ts-node --esm server.ts",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.tsx && cross-env NODE_ENV=production tsc -p tsconfig.server.json",
    "build": "yarn build:client && yarn build:server",
    "start": "lerna run build && cross-env NODE_ENV=production node ./dist/server",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest --config jest.config.ts ./",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.1.0",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "nanoid": "^5.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-hook-form": "7.45.4",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^28.1.8",
    "@types/node": "^20.11.19",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.33",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "classnames": "^2.5.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.0.1",
    "jest-transformer-svg": "^2.0.2",
    "lefthook": "^1.3.9",
    "prettier": "^2.7.1",
    "react-hook-form": "7.45.4",
    "sass": "^1.69.7",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "^3.0.7",
    "vite-plugin-svgr": "^4.2.0"
  },
  "license": "MIT"
}
